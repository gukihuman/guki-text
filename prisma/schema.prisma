generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String @unique
  userData String
}

model Collision {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String @unique
  collisionData String
}
